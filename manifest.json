{
    "name": "SPO View It",
    "version": "1.1",
    "description": "Force SharePoint Online to Reading View",
    "background": {
        "scripts": [ "background.js" ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://*.sharepoint.com/*"
      ],
    "icons": { "16": "icon16.png", "32": "icon32.png", "48": "icon48.png", "128": "icon128.png" },
    "manifest_version": 2
}
